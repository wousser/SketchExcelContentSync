  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Wouter Bron <wousser@gmail.com>",
  "dependencies": {
    "@skpm/dialog": "^0.2.4",
    "@skpm/fs": "^0.2.2",
    "csvtojson": "^2.0.8",
    "xlsx": "^0.14.0"
  }
}
