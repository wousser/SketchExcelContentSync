{
  "name": "excel-content-sync",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Excel Content Sync",
    "manifest": "src/manifest.json",
    "main": "excelContentSync.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
